{
    "version": 2,
    "builds": [
      {
        "src": "App/wsgi.py",
        "use": "@vercel/python",
        "config": { 
          "maxLambdaSize": "15mb",
          "runtime": "python3.12.7"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "App/wsgi.py"
      }
    ]
  }